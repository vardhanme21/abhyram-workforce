<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Edit_Approved_Timesheet</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Status__c), 'Approved'), NOT(ISNEW()))</errorConditionFormula>
    <errorMessage>Cannot edit an approved timesheet</errorMessage>
</ValidationRule>